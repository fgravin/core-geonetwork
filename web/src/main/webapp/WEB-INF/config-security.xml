    <bean class="org.springframework.security.access.hierarchicalroles.RoleHierarchyImpl" id="roleHierarchy">
        <property name="hierarchy">
            <value>
                ROLE_Administrator > ROLE_UserAdmin
ROLE_Administrator > ROLE_Monitor
ROLE_UserAdmin > ROLE_Reviewer
ROLE_Reviewer > ROLE_Editor
ROLE_Editor > ROLE_RegisteredUser
ROLE_RegisteredUser > ROLE_Guest
ROLE_Monitor > ROLE_RegisteredUser
            </value>
        </property>
    </bean>
    <bean class="org.springframework.security.access.vote.AffirmativeBased" id="accessDecisionManager">
        <property name="decisionVoters">
            <list>
                <bean class="org.springframework.security.access.vote.RoleHierarchyVoter">
                    <constructor-arg ref="roleHierarchy"></constructor-arg>
                </bean>
                <bean class="org.springframework.security.access.vote.AuthenticatedVoter"></bean>
            </list>
        </property>
    </bean>
