  <skos:Concept
          rdf:about="http://www.seadatanet.org/emodnet/measuring-instrument#SDN:L054:12:130">
    <skos:prefLabel xml:lang="en">CTD</skos:prefLabel>
    <skos:definition xml:lang="en">A package lowered and raised vertically from
      a surface platform, usually a ship, always carrying pressure, temperature
      and conductivity sensors. Addional optional sensors include fluorometers,
      transmissometers and radiometers. The package may also carry water
      bottles.
    </skos:definition>
    <skos:prefLabel xml:lang="fr">CTD</skos:prefLabel>
    <skos:definition xml:lang="fr">A package lowered and raised vertically from
      a surface platform, usually a ship, always carrying pressure, temperature
      and conductivity sensors. Addional optional sensors include fluorometers,
      transmissometers and radiometers. The package may also carry water
      bottles.
    </skos:definition>
  </skos:Concept>
  <skos:Concept
          rdf:about="http://www.seadatanet.org/emodnet/measuring-instrument#SDN:L054:12:131">
    <skos:prefLabel xml:lang="en">CTD undulators</skos:prefLabel>
    <skos:definition xml:lang="en">A package towed at varying depths by a ship,
      always carrying pressure, temperature and conductivity sensors. Addional
      optional sensors include fluorometers, transmissometers and radiometers.
    </skos:definition>
    <skos:prefLabel xml:lang="fr">CTD undulators</skos:prefLabel>
