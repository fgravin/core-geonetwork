  <skos:Concept
          rdf:about="http://www.seadatanet.org/emodnet/measuring-instrument#SDN:L054:12:388">
    <skos:prefLabel xml:lang="en">flow meters</skos:prefLabel>
    <skos:definition xml:lang="en">Sensors that quantify the rate at which
      fluids (e.g. water or air) pass through sensor packages, instruments or
      sampling devices.
    </skos:definition>
    <skos:prefLabel xml:lang="fr">flow meters</skos:prefLabel>
