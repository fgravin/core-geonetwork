	 <element name="gmd:code" id="207.0"
        context="gmd:MD_Metadata/gmd:referenceSystemInfo/gmd:MD_ReferenceSystem/gmd:referenceSystemIdentifier/gmd:RS_Identifier/gmd:code">
        <label>Coordinate reference system</label>
        <description>Alphanumeric value identifying an instance in the namespace.</description>
        <help>
        </help>
        <condition>mandatory</condition>
        <helper>            
            <option value="WGS 84 (EPSG 4326)">WGS 84 (EPSG 4326)</option>
            <option value="WGS 84 / Simple Mercator (EPSG 41001)">WGS 84 / Simple Mercator (EPSG 41001)</option>
            <option value="Lambert Azimuthal Equal Area (EPSG 3035)">Lambert Azimuthal Equal Area (EPSG 3035)</option>
            <option value="ETRS89 (EPSG 4258)">ETRS89 (EPSG 4258)</option>
            <option value="WGS 84 / Plate Carree (EPSG 32662)">WGS 84 / Plate Carree (EPSG 32662)</option>
            <option value="WGS 84 / World Mercator (EPSG 3395)">WGS 84 / World Mercator (EPSG 3395)</option>
            <option value="MYO Arakawa-C ORCA025 native grid">MYO Arakawa-C ORCA025 native grid</option>
            <option value="NSIDC Sea Ice Polar Stereographic North (EPSG 3411)">NSIDC Sea Ice Polar Stereographic North (EPSG 3411)</option>
            <option value="NSIDC Sea Ice Polar Stereographic South (EPSG 3412)">NSIDC Sea Ice Polar Stereographic South (EPSG 3412)</option>
            <option value="Spherical 3D CS (EPSG 6404)">Spherical 3D CS (EPSG 6404)</option>
            <option value="WRF Lambert Conformal Conic (SR-ORG 29)">WRF Lambert Conformal Conic (SR-ORG 29)</option>
        </helper>
    </element>
    <element name="gmd:resolution">
        <label>Temporal resolution</label>
        <description></description>
		<help/>
    </element>
  	<element name="gmd:protocol" id="398.0">
		<label>Protocol</label>
		<description>Connection protocol to be used</description>
		<helper sort="true">
			<option show="-" value="OGC:WMS">Web Map Service (WMS)</option>
			<option show="-" value="WWW:DOWNLOAD-1.0-link-download">Download (link)</option>
		</helper>
	</element>
